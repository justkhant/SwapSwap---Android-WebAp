<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="edu.upenn.cis350.final_project">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <activity
            android:name=".LoginActivity"
            android:label="Login">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".UserProfileActivity"></activity>
        <activity android:name=".EditProfileActivity"></activity>
        <activity android:name=".SignupActivity"></activity>
        <activity android:name=".InstructionsActivity"></activity>
        <activity android:name=".ItemListAdapter"></activity>
        <activity android:name=".HomeActivity"></activity>
    </application>
    <uses-permission android:name="android.permission.INTERNET"/>
</manifest>