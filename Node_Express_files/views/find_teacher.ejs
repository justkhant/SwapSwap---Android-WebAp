<head>
  <title>Find Teacher</title>
  <link rel="stylesheet" type="text/css" href="public/css/find_teacher.css">
</head>
<body>
  <%- include ('partials/header') %>
  <div class="header">
    <div class="card">
      <img src="<%= 'data:image/bmp;base64,' + data.profilePic %>" width="100" height="100" />
      <h1><%= data.name %></h1>
      <p class="info" id="email_to_be_deleted">Email: <%= data.email %></p>
      <p class="info">Phone Number: <%= data.phoneNumber %></p>
      <p class="info">School: <%= data.school %></p>
      <p class="info">Bio: <%= data.bio %></p>
    </div>
  </div>
</body>

